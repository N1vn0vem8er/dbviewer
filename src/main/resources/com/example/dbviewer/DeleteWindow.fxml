<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.dbviewer.DeleteWindow"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <SplitPane dividerPositions="0.3" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <ListView fx:id="tableList" onMousePressed="#tableSelected" prefHeight="200.0" prefWidth="200.0" />
                <VBox fx:id="fieldContainer" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <Label text="Select table" />
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </children>

</AnchorPane>
